<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.snowline.spzx.manager.mapper.ProductUnitMapper">



<!--    //查询所有单位-->
    <select id="findAll" resultType="com.snowline.spzx.model.entity.base.ProductUnit">
        select *
        from product_unit pu
        where pu.is_deleted = 0
        order by pu.id desc

    </select>


</mapper>